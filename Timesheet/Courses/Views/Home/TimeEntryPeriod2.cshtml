<script type="text/ng-template" id="addTimeRows.html">

    <div class="modal-header">

        <h4 class="modal-title">{{modalTitle}}</h4>
    </div>
    <div class="modal-body">

        <form class="form-horizontal" name="client">
            <div class="col-12">


                <div class="col-sm-12">

                    <label>Company Name</label>
                    <select class="form-control" id="selectedCompany" ng-change="changeCompany()" ng-model="selectedCompany">
                        <option ng-repeat="x in Obj_getCompany" value="{{x.CompanyId}}">{{x.CompanyName}}</option>
                    </select>
                </div>
                <br />
                <div class="col-sm-12">

                    <label>Company Project</label>
                    <select class="form-control" id="selectedProject" ng-model="selectedProject">
                        <option ng-repeat="x in Obj_getProjects" value="{{x.ProjectId}}">{{x.ProjectName}}</option>
                    </select>
                </div>
                <br />
                <div class="col-sm-12">

                    <label>Working Hours</label>
                    <input type="text" ng-model="hours" class="form-control" />
                </div>
                <br />
                <div class="col-sm-12">

                    <label>Current Date</label>
                    <select class="form-control" ng-model="selectedCurrentDate" id="selectedCurrentDate">
                        <option ng-repeat="x in datesdata" value="{{x | date: 'MM/dd/yyyy'}}">{{x | date : "EEE, MMMM d, y"}}</option>
                    </select>
                </div>

            </div>

            <br />
            <div class="alert alert-success alert-block" ng-show="isSuccess">

                <h6 class="alert-heading">{{successMessage}}</h6>
            </div>
            <div class="alert alert-danger alert-block" ng-show="isError">
                <h6 class="alert-heading">{{errormessage}}</h6>
            </div>

        </form>

    </div>
    <div class="modal-footer">
        <button class="btn btn-primary" type="button" ng-click="addWorkingHours()">Save</button>
        <button class="btn btn-warning" type="button" ng-click="cancel()">Cancel</button>
    </div>

</script>

<!-- /.content --> 




