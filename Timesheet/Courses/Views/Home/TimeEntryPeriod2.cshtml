
@{
    ViewBag.Title = "TimeEntryPeriod2";
    Layout = "~/Views/Shared/Timesheet_Layout.cshtml";
}

<section class="content-header sty-one">
    <h1 class="text-black">Time Entry Period2</h1>
    <ol class="breadcrumb">
        <li><a href="/Home/Index">Home</a></li>
        
        <li><i class="fa fa-angle-right"></i> Time Entry Period2</li>
    </ol>
</section>
<input type="hidden" id="paramid" value="@ViewBag.id"  />
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    @*<button  type="button" class="btn btn-sm btn-success"><i class="fa fa-plus"></i> Add TimeSheet</button>*@
                 
                        <div>
                            <button type="button" class="btn btn-light"><i class="fa fa-list-ul"></i> <span>Day View</span></button>
                            <button type="button" class="btn btn-light"><i class="fa fa-calendar"></i> <span>Timesheet Periods</span></button>&nbsp;
                            <button type="button" class="btn btn-success"><i class="fa fa-pencil"></i> <span id="C_C_C_W_spnTopSave">Save</span></button>&nbsp;

                            <button type="button" class="btn btn-info"><i class="fa fa-pencil-square-o"></i> <span>Submit</span></button>&nbsp;
                            <button type="button" class="btn btn-light"><i class="fa fa-tachometer"></i> <span>Start Timer</span></button>
                            <button type="button" class="btn btn-light"><i class="fa fa-clipboard"></i> <span id="C_C_C_W_spnAudit">Audit</span></button>
                        </div>
                    <hr >
                    <div class="form-group">
                        <div class="row" style="margin-left: 0; margin-right: 0;">
                            <div class="col-lg-3">
                                <h4 style="font-size:15px;" class="text-black"><i class="fa fa-user-o"></i>&nbsp;Employee</h4>

                                <input class="form-control" type="text" readonly value="{{UserName}}">

                                @*<select class="form-control">
                                    <option>{{UserName}}</option>
                               

                                  
                                </select>*@

                            </div>

                            <div class="col-lg-3">
                                <h4 style="font-size:14px;margin-bottom:10px;" class="text-black"><i class="fa fa-calendar"></i>&nbsp;Current Time Sheet Period</h4>
                                <input class="form-control" type="text" readonly  value="{{Obj_getTimePeriodsPerId[0].TimePeriods}}">
                            </div>

                            <div class="col-lg-3">
                                <h4 style="font-size:15px;" class="text-black"><i class="fa fa-plus"></i>&nbsp;TimeSheet Total</h4>
                                <span style="font-size: x-large;color: #333333;padding: 42px;">{{Obj_getTimePeriodsPerId[0].Hours}}</span>
                            </div>

                            <div class="col-lg-3">
                                <h4 style="font-size:15px;" class="text-black"><i class="fa fa-th"></i>&nbsp;TimeSheet Status</h4>
                                <span ng-show="Obj_getTimePeriodsPerId[0].Status == 'Not Submitted'" class="label label-warning "style="border-radius: 10px;padding: 3px 10px;font-size: 14px;margin-left: 20px;">Not Submitted</span>
                                <span ng-show="Obj_getTimePeriodsPerId[0].Status == 'Submitted'" class="label label-info " style="border-radius: 10px;padding: 3px 10px;font-size: 14px;margin-left: 20px;">Submitted</span>
                            </div>

                        </div>
                   </div>
              </div>
           </div>
        </div>
    </div>
</section>


